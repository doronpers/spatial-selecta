{
  "name": "spatialselects",
  "version": "1.0.0",
  "description": "SpatialSelects.com - A website that automatically updates when new music becomes available in Dolby Atmos spatial audio format on Apple Music",
  "main": "index.html",
  "scripts": {
    "start": "npx http-server . -p 8080",
    "backend": "uvicorn backend.main:app --reload --port 8000",
    "backend:prod": "uvicorn backend.main:app --host 0.0.0.0 --port 8000",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "spatial-audio",
    "dolby-atmos",
    "apple-music",
    "immersive-audio",
    "360-reality-audio",
    "music-discovery"
  ],
  "author": "SpatialSelects.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/doronpers/spatial-selecta.git"
  },
  "bugs": {
    "url": "https://github.com/doronpers/spatial-selecta/issues"
  },
  "homepage": "https://spatialselects.com",
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
